{
  "name": "teamfuse",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx server.ts",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint",
    "seed": "dotenv -e .env.local -- node prisma/seed.js",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "sync:worker" : "dotenv -e .env.local -- tsx src/lib/queue/syncWorker.ts",
    "webhook:worker" : "dotenv -e .env.local -- tsx src/lib/queue/webhookWorker.ts",
    "sync_worker" : "tsx src/lib/queue/syncWorker.ts",
    "webhook_worker" : "tsx src/lib/queue/webhookWorker.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "bullmq": "^5.65.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "jose": "^6.1.1",
    "lucide-react": "^0.552.0",
    "next": "^16.0.1",
    "next-auth": "^4.24.13",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "recharts": "^3.5.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --ignore-pattern \"src/generated/**\" --fix",
      "prettier --write"
    ]
  } 
}
